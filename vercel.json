{
  "version": 2,
  "rewrites": [
  // 1. Проброс команд к вашему движку (скрытая часть)
    {
      "source": "/api/subsexxo1/:path*",
      "destination": "/api/index.js"
    },
  // 2. Отображение интерфейса (красивая часть)
    {
      "source": "/(.*)",
      "destination": "https://sub-store.vercel.app/$1"
    }
  ]
}
